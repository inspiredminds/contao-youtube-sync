{
    "name": "inspiredminds/contao-youtube-sync",
    "description": "Contao extension to synchronize a contao news archive with a YouTube playlist.",
    "type": "contao-bundle",
    "license": "LGPL-3.0-or-later",
    "homepage": "https://github.com/inspiredminds/contao-youtube-sync",
    "authors": [
        {
            "name": "Fritz Michael Gschwantner",
            "email": "fmg@inspiredminds.at",
            "role": "Developer"
        }
    ],
	"support": {
		"issues": "https://github.com/inspiredminds/contao-news-navigation/issues",
		"source": "https://github.com/inspiredminds/contao-news-navigation",
		"forum": "https://community.contao.org/de"
	},
	"funding": [
        {
            "type": "github",
            "url": "https://github.com/sponsors/fritzmg"
        }
    ],
    "require": {
        "php": ">=8.2",
        "contao/core-bundle": "^4.13 || ^5.0",
        "contao/news-bundle": "^4.13 || ^5.0",
        "doctrine/dbal": "^3.0 || ^4.0",
        "google/apiclient": "^2.18",
        "guzzlehttp/guzzle": "^6.0 || ^7.0",
        "symfony/config": "^5.4 || ^6.4 || ^7.3",
        "symfony/console": "^5.4 || ^6.4 || ^7.3",
        "symfony/dependency-injection": "^5.4 || ^6.4 || ^7.3",
        "symfony/event-dispatcher-contracts": "^3.6",
        "symfony/http-foundation": "^5.4 || ^6.4 || ^7.3",
        "symfony/http-kernel": "^5.4 || ^6.4 || ^7.3",
        "symfony/routing": "^5.4 || ^6.4 || ^7.3",
        "symfony/translation-contracts": "^3.6",
        "twig/twig": "^3.21"
    },
    "autoload": {
        "psr-4": {
            "InspiredMinds\\ContaoYouTubeSync\\": "src/"
        }
    },
    "extra": {
        "contao-manager-plugin": "InspiredMinds\\ContaoYouTubeSync\\ContaoManager\\Plugin"
    },
    "require-dev": {
        "contao/easy-coding-standard": "^6.0",
        "contao/rector": "^1.0",
        "shipmonk/composer-dependency-analyser": "^1.8"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "contao/manager-plugin": true,
            "contao-components/installer": true,
            "php-http/discovery": false,
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
